@model IEnumerable<Item>

@{
    ViewData["Title"] = "Index";
}

<h1>Item Index</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Item Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model){
            <tr>
                <td width=50%>
                    @obj.Name
                </td>
                <td width=30%>
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>
